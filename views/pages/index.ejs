<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/styles/style.css" />
    <title>Search for a Book</title>
  </head>
  <body>
    <header>
      <div class="headerTop">
        <div class="logo">Best Books!</div>
        <div class="hamburgerMenu">
          <div class="line"></div>
          <div class="line"></div>
          <div class="line"></div>
        </div>
      </div>
      <nav class="none">
        <ul>
          <li><a href="/">Home Page</a></li>
          <li><a href="/searches/new">Search</a></li>
        </ul>
      </nav>
    </header>
    <main>
      <h1 class="total_saved_books">Total saved books: <%= totalCount %> </h1>
      <% books.forEach(book => { %>
        <div class="bookCard">
          <img src="<%= book.image %> " alt="" />
          <h2 class="book_title"><%= book.title %></h2>
          <p class="book_auther"><%= book.auther %></p>
  
          <a class="book_view_detail_btn" href="#">View Details</a>
        </div>
        <% }) %>
    </main>

    <footer></footer>
    <script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>
    <script src="/js/app.js"></script>
  </body>
</html>
